<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
	<record id="sh_message_wizard" model="ir.ui.view">
		<field name="name">sh.message.wizard</field>
		<field name="model">sh.message.wizard</field>
		<field name="arch" type="xml">
			<form string="Message">
				<field name="name"/>
				<footer>
					<button string="OK" class="btn-primary" special="cancel" />
				</footer>
			</form>
		</field>	
	</record>
	  <record id="sh_message_wizard_search_view" model="ir.ui.view">
            <field name="name">sh.message.wizard</field>
            <field name="model">sh.message.wizard</field>
            <field name="arch" type="xml">
                <search string="Search Meassage">
                    <field name="name" string="Message"/>
                </search>
            </field>
        </record>
</data>
</odoo>